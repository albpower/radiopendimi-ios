<?xml version='1.0' encoding='utf-8'?>
<widget id="com.radiopendimi.app" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Radio Pendimi</name>
    <description>
        Radio Pendimi - radio islame ne gjuhen shqipe
    </description>
    <author email="info@radio-pendimi.com" href="http://radio-pendimi.com">
       Afrim Azizi
    </author>
    
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.6.3" />
    <preference name="orientation" value="default" />
	<preference name="AutoHideSplashScreen" value="false" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
   
    
    <icon src="icon.png" />
	<!-- iOS Icons  -->

        <!-- 7.0+ -->
        <icon src="res/icon/ios/AppIcon29x29.png"     gap:platform="ios"        width="29" height="29" />
        <icon src="res/icon/ios/AppIcon40x40.png"        gap:platform="ios"        width="40" height="40" />
        <icon src="res/icon/ios/AppIcon57x57.png"        gap:platform="ios"        width="57" height="57" />
        <icon src="res/icon/ios/AppIcon29x29@2x.png"  gap:platform="ios"        width="58" height="58" />
        <icon src="res/icon/ios/AppIcon60x60.png"        gap:platform="ios"        width="60" height="60" />
        <icon src="res/icon/ios/AppIcon76x76.png"        gap:platform="ios"        width="76" height="76" />
        <icon src="res/icon/ios/AppIcon40x40@2x.png"     gap:platform="ios"        width="80" height="80" />
        <icon src="res/icon/ios/AppIcon60x60@2x.png"     gap:platform="ios"        width="120" height="120" />       
        <icon src="res/icon/ios/AppIcon76x76@2x.png"     gap:platform="ios"        width="152" height="152" />
		<!-- 8.0+ -->  
		<icon src="res/icon/ios/AppIcon180x180.png"     gap:platform="ios"        width="180" height="180" />  
		
        <!-- 6.1 -->
        <icon src="res/icon/ios/AppIcon50x50.png"        gap:platform="ios"        width="50" height="50" />
        <icon src="res/icon/ios/AppIcon72x72.png"        gap:platform="ios"        width="72" height="72" />     
        <icon src="res/icon/ios/AppIcon50x50@2x.png"     gap:platform="ios"        width="100" height="100" />
        <icon src="res/icon/ios/AppIcon57x57@2x.png"     gap:platform="ios"        width="114" height="114" />
        <icon src="res/icon/ios/AppIcon72x72@2x.png"     gap:platform="ios"        width="144" height="144" />
	
	<gap:splash src="splash.png" />
    <!-- iOS Splash Screens -->
    <gap:splash src="res/splash/ios/Default.png"                       gap:platform="ios"       width="320" height="480" />
    <gap:splash src="res/splash/ios/Default@2x.png"                    gap:platform="ios"       width="640" height="960" />
    <gap:splash src="res/splash/ios/Default-568h@2x.png"               gap:platform="ios"       width="640" height="1136" />
	<gap:splash src="res/splash/ios/Default-1242x2208.png"               gap:platform="ios"       width="1242" height="2208" />
	
	<!-- Plugin for background audio playing -->
   <gap:plugin name="nl.kingsquare.background-audio" />
   <!-- Pushwoosh plugin -->
   <gap:plugin name="com.pushwoosh.plugins.pushwoosh"  source="plugins.cordova.io" />
   <gap:plugin name="org.apache.cordova.inappbrowser" />
   <gap:plugin name="org.apache.cordova.device" />
    
    <access origin="*" />
</widget>